<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ets.echo.sign.client.class">ETS\EchoSignBundle\Api\Client</parameter>
    </parameters>

    <services>
        <service id="ets.echo.sign.client" class="%ets.echo.sign.client.class%">
            <argument>%ets.echo.sign.api.key%</argument>
            <argument>%ets.echo.sign.api.gateway%</argument>
            <argument>%ets.echo.sign.api.wsdl%</argument>
        </service>
    </services>
</container>
